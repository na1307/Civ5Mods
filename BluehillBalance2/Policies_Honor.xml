<!-- Created by ModBuddy on 2024-09-10 오후 7:11:33 -->
<GameData>
  <Policies>
    <Update>
      <Set>
        <AlwaysSeeBarbCamps>false</AlwaysSeeBarbCamps>
      </Set>
    </Update>
    <Update>
      <Where Type="POLICY_HONOR" />
      <Set>
        <CultureFromKills>100</CultureFromKills>
        <CultureFromBarbarianKills>0</CultureFromBarbarianKills>
      </Set>
    </Update>
    <Update>
      <Where Type="POLICY_WARRIOR_CODE" />
      <Set>
        <IncludesOneShotFreeUnits>false</IncludesOneShotFreeUnits>
        <GreatGeneralRateModifier>0</GreatGeneralRateModifier>
      </Set>
    </Update>
    <Update>
      <Where Type="POLICY_MILITARY_TRADITION" />
      <Set>
        <IncludesOneShotFreeUnits>true</IncludesOneShotFreeUnits>
        <ExpModifier>0</ExpModifier>
      </Set>
    </Update>
    <Update>
      <Where Type="POLICY_PROFESSIONAL_ARMY" />
      <Set>
        <GoldFromKills>100</GoldFromKills>
      </Set>
    </Update>
    <Update>
      <Where Type="POLICY_HONOR_FINISHER" />
      <Set>
        <GoldFromKills>0</GoldFromKills>
        <GreatGeneralRateModifier>50</GreatGeneralRateModifier>
        <ExpModifier>50</ExpModifier>
      </Set>
    </Update>
  </Policies>
  <Policy_BuildingClassProductionModifiers>
    <Update>
      <Where PolicyType="POLICY_PROFESSIONAL_ARMY" />
      <Set>
        <PolicyType>POLICY_WARRIOR_CODE</PolicyType>
      </Set>
    </Update>
  </Policy_BuildingClassProductionModifiers>
  <Policy_Flavors>
    <Update>
      <Where PolicyType="POLICY_WARRIOR_CODE" FlavorType="FLAVOR_MILITARY_TRAINING" />
      <Set>
        <Flavor>15</Flavor>
      </Set>
    </Update>
    <Update>
      <Where PolicyType="POLICY_WARRIOR_CODE" FlavorType="FLAVOR_GREAT_PEOPLE" />
      <Set>
        <PolicyType>POLICY_MILITARY_TRADITION</PolicyType>
      </Set>
    </Update>
    <Update>
      <Where PolicyType="POLICY_PROFESSIONAL_ARMY" FlavorType="FLAVOR_MILITARY_TRAINING" />
      <Set>
        <Flavor>5</Flavor>
      </Set>
    </Update>
    <Update>
      <Where PolicyType="POLICY_PROFESSIONAL_ARMY" FlavorType="FLAVOR_GOLD" />
      <Set>
        <Flavor>25</Flavor>
      </Set>
    </Update>
    <Delete PolicyType="POLICY_HONOR_FINISHER" FlavorType="FLAVOR_GOLD" />
    <Row>
      <PolicyType>POLICY_HONOR_FINISHER</PolicyType>
      <FlavorType>FLAVOR_MILITARY_TRAINING</FlavorType>
      <Flavor>20</Flavor>
    </Row>
  </Policy_Flavors>
  <Policy_UnitCombatProductionModifiers>
    <Update>
      <Where PolicyType="POLICY_WARRIOR_CODE" />
      <Set>
        <ProductionModifier>30</ProductionModifier>
      </Set>
    </Update>
  </Policy_UnitCombatProductionModifiers>
  <Policy_FreeUnitClasses>
    <Update>
      <Where PolicyType="POLICY_WARRIOR_CODE" UnitClassType="UNITCLASS_GREAT_GENERAL" />
      <Set>
        <PolicyType>POLICY_MILITARY_TRADITION</PolicyType>
      </Set>
    </Update>
  </Policy_FreeUnitClasses>
</GameData>